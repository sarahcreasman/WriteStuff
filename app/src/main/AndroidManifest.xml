<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sarahcreasman.writestuff">
    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ToDoList">
            <intent-filter>
                <action android:name="com.sarahcreasman.writestuff.ToDoList" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Notebook">
            <intent-filter>
                <action android:name="com.sarahcreasman.writestuff.Notebook" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".NotebookPage">
            <intent-filter>
                <action android:name="com.sarahcreasman.writestuff.NotebookPage" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Timer">
            <intent-filter>
                <action android:name="com.sarahcreasman.writestuff.Timer" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".NameDictionary">
            <intent-filter>
                <action android:name="com.sarahcreasman.writestuff.NameDictionary" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Feedback">
            <intent-filter>
                <action android:name="com.sarahcreasman.writestuff.Feedback" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".NameMales">
            <intent-filter>
                <action android:name="com.sarahcreasman.writestuff.NameMales" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".NameFemales">
            <intent-filter>
                <action android:name="com.sarahcreasman.writestuff.NameFemales" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".NameNeutral">
            <intent-filter>
                <action android:name="com.sarahcreasman.writestuff.NameNeutral" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>